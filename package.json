{
  "name": "@ng2react/core",
  "version": "0.0.0-alpha",
  "description": "Convert AngularJS 1.x components to React components",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "files": [
    "dist",
    "src/lib"
  ],
  "scripts": {
    "preversion": "npm run lint && npm run test && npm run build",
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist",
    "build": "npm run build-cjs && npm run build-esm",
    "build-esm": "tsc --build ./src/lib/tsconfig.json",
    "build-cjs": "tsc --build ./src/lib/tsconfig.cjs.json",
    "test": "cross-env mocha",
    "lint": "eslint **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxbilbow/ng2react-core.git"
  },
  "author": "Max Bilbow",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxbilbow/ng2react-core/issues"
  },
  "homepage": "https://github.com/maxbilbow/ng2react-core#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "openai": "^3.2.1",
    "tslog": "^4.8.2",
    "typescript": "^5.0.2",
    "yargs": "^17.7.1"
  },
  "overrides": {
    "typescript": "$typescript"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/register": "^7.21.0",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "mocha": "^10.2.0",
    "rimraf": "^4.4.0",
    "rollup": "^3.18.0",
    "ts-node": "^10.9.1"
  }
}
